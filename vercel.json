{
    "version": 2,
    "functions": {
      "api/index.php": { "runtime": "@vercel/php@0.6.0" }
    },
    "routes": [
      {
        "src": "/(css|js|images)/(.*)",
        "dest": "public/$1/$2"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://your-project.vercel.app"
    }
  }